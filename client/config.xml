<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.cs410tsgj.letsdothis" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LetsDoThis</name>
    <description>
        The worlds greatest event planner.
    </description>
    <author email="y4n8@ugrad.cs.ubc.ca" href="http://cordova.io">
        CPSC 410 TSGJ
    </author>
    <content src="login.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
